{
  "name": "@createconomy/utils",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./format": {
      "import": "./dist/format.js",
      "types": "./src/format.ts"
    },
    "./validation": {
      "import": "./dist/validation.js",
      "types": "./src/validation.ts"
    },
    "./tenant": {
      "import": "./dist/tenant.js",
      "types": "./src/tenant.ts"
    },
    "./logger": {
      "import": "./dist/logger.js",
      "types": "./src/logger.ts"
    },
    "./errors": {
      "import": "./dist/errors.js",
      "types": "./src/errors.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.2"
  }
}
